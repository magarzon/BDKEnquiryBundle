    "description": "Bundle to handle enquiries and their answers",
    "keywords": ["enquiry"],
    "homepage": "http://bodaclick.github.com",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1,<2.3-dev",
        "symfony/serializer": "2.1.*@dev"
    },
    "require-dev": {
    	"doctrine/doctrine-bundle": "*",
    	"doctrine/mongodb-odm-bundle": "3.0.*",
    	"doctrine/orm": ">=2.2.3,<2.4-dev"
    },
    
    "autoload": {
        "psr-0": { "Bodaclick\\BDKEnquiryBundle": "" }
    },
    "target-dir": "Bodaclick/BDKEnquiryBundle"
}
